"#Wrapper": {
	layout: "vertical"
},
"#container": {
	top: "0dp",
	backgroundColor: "#000",
	contentWidth: "auto",
	contentHeight: "auto",
	layout: "vertical"
},
"Label": {
	color: "#666",
	font: {
		fontSize: "14dp",
		fontFamily: "HelveticaNeue"
	},
	top: "5dp",
	left: "15dp",
	right: "15dp",
	height: Ti.UI.SIZE,
	textAlign: "left"
},
"#copyright": {
	top: "20dp"
},
"#chariti": {
	bottom: "20dp"
},
"#alertmessage": {
	width: Titanium.UI.FILL,
	height: Ti.UI.SIZE,
	width: "300dp"
	
},
"#message": {
	color: 'darkgray',
